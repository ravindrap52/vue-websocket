{
  "name": "trade-republic-challenge",
  "private": true,
  "version": "0.1.1",
  "scripts": {
    "dev": "concurrently \"node server.mjs\" \"vite\"",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "concurrently \"node server.mjs\" \"vite preview\"",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.0.35",
    "vue": "^3.2.41",
    "vue3-toastify": "^0.1.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "concurrently": "^7.5.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.55.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.4",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4",
    "ws": "^8.10.0"
  }
